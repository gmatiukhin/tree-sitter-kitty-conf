{
  "name": "kittyconf",
  "rules": {
    "varstring_part": {
      "type": "PATTERN",
      "value": "[^$]*"
    },
    "glob_pattern": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "glob_pattern_star"
          },
          {
            "type": "SYMBOL",
            "name": "glob_pattern_qmark"
          },
          {
            "type": "SYMBOL",
            "name": "glob_pattern_literal"
          },
          {
            "type": "SYMBOL",
            "name": "glob_pattern_literal_glob_op"
          },
          {
            "type": "SYMBOL",
            "name": "glob_pattern_seq"
          }
        ]
      }
    },
    "glob_pattern_star": {
      "type": "STRING",
      "value": "*"
    },
    "glob_pattern_qmark": {
      "type": "STRING",
      "value": "?"
    },
    "glob_pattern_literal_glob_op": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "[*]"
        },
        {
          "type": "STRING",
          "value": "[?]"
        }
      ]
    },
    "glob_pattern_seq": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": "!"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "PATTERN",
          "value": "[^\\]]+"
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "glob_pattern_literal": {
      "type": "PATTERN",
      "value": "[^\\*\\?\\[]+"
    },
    "string": {
      "type": "PATTERN",
      "value": "\\S.+"
    },
    "layout_option_atom": {
      "type": "PATTERN",
      "value": "[^\\s;:=,]+"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "PATTERN",
        "value": "#.*\\n"
      }
    }
  },
  "extras": [],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
